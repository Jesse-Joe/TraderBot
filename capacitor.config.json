{
  "appId": "com.phantrium.traderbot",
  "appName": "TraderBot",
  "webDir": "www",
  "server": {
    "url": "https://trader.phantrium.com",
    "cleartext": false
  },
  "ios": {
    "contentInset": "always",
    "allowsLinkPreview": false,
    "scrollEnabled": true,
    "limitsNavigationsToAppBoundDomains": true,
    "backgroundColor": "#0a0a0f"
  },
  "android": {
    "allowMixedContent": false,
    "backgroundColor": "#0a0a0f"
  },
  "plugins": {
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#0a0a0f",
      "showSpinner": false
    }
  }
}
